.exam_btn_group {
    position: fixed;
    bottom: 0;
    border: 1px solid gainsboro;
    background-color: white;
    width: 100%;
    margin: 0;
    z-index: 998;
}

.bg-green {
    background-color: #1e90ff;
}

.bg-white {
    height: 200rpx;
}

.placeholder {
    height: 50rpx;
}

.cu-modal {
    height: 100%;
    z-index: 9998;
}

.cu-modal .cu-dialog {
    z-index: 9999;
}

.modal_btn {
    width: 100%;
    height: 70%;
    display: flex;
    justify-content: space-between;
}

.modal_btn .but {
    border: 1px solid gainsboro;
    height: 100%;

}

.text-green,
.line-green,
.lines-green {
    color: #1e90ff;
}

.subject {
    height: 100%;
    padding-bottom: 50rpx;
    margin-top: 20rpx;
    margin-bottom: 50rpx;
    background-color: white;
}

.selects {
    border: 1px solid gainsboro;
    margin: 20rpx;
    border-radius: 5px;

}

.select {
    border: 1px solid #1e90ff;
    background-color: #1e8fff25;
}

.error {
    border: 1px solid #ff1e1e;
    background-color: #ff1e1e23;
}
.answer{
    margin-left: 20rpx;
}