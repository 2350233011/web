.data_body {
    background-color: white;
    margin: 20rpx;
    border-radius: 10rpx;
}

.goods_num {
    width: 100rpx;
}

.input_data {
    margin-top: 20rpx;
    display: flex;
    flex-direction: column;
    height: 90vh;
    overflow: hidden;
}

.gotop {
    position: absolute;
    top: 85%;
    left: 85%;
}

.gotop text {
    font-size: 40rpx;
    font-weight: 800;
    padding: 20rpx;
    background-color: white;
    border-radius: 50%;
    border: 1rpx solid gainsboro;
}

.subordinate {
    display: flex;
    flex-direction: column;
}

.subordinate text {
    margin-bottom: -10rpx;
}

.data_body2 {
    margin: 10rpx 40rpx;
    border-radius: 10rpx;
    background-color: #dcdcdc4d;
}

.receiving {
    background-color: #dcdcdc4d;
    padding: 10rpx 30rpx;
}

.clarify_top .cu-btn{
    margin-left: 10rpx;
}
.text-important{
    color: red;
    width: 20rpx;
}